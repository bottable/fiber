{
  "name": "fiber-ui",
  "version": "1.0.24",
  "description": "A React UI Component library",
  "author": "bottable",
  "license": "MIT",
  "repository": "bottable/fiber",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently 'yarn run dev' 'yarn run docs:dev'",
    "dev": "microbundle-crl watch --no-compress --format modern,cjs",
    "build": "yarn clean && yarn build:pkg",
    "build:pkg": "rollup -c",
    "build:docs": "cd ./website && yarn && yarn build",
    "prepublishOnly": "yarn run build",
    "clean": "rimraf ./dist",
    "docs:dev": "cd ./website && yarn start",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 jest --env=jsdom",
    "test:watch": "cross-env CI=1 jest",
    "test:refresh": "jest --updateSnapshot --env=jsdom",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "yarn run lint:js -- --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "stylelint --config=.stylelintrc --fix",
      "pretty-quick --staged"
    ],
    "src/**/*.scss": [
      "stylelint --config=.stylelintrc --syntax scss --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-icons": "^3.11.0",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "hygen": "^5.0.3",
    "lint-staged": "^10.2.4",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-helmet-async": "^1.0.6",
    "react-icons": "^3.11.0",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-typescript2": "^0.27.3",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5",
    "stylelint": "^13.5.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^4.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "framer": "^1.2.15",
    "polished": "^3.6.4",
    "rc-util": "^5.0.6",
    "react-resize-detector": "^6.0.1-rc.3"
  }
}
